{
  "name": "tenk",
  "version": "1.0.0",
  "main": "dist/cli.js",
  "author": "Benjamin Tatum",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^18.7.16",
    "@types/yargs": "^17.0.12",
    "tsup": "^6.2.3"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch --onSuccess \"node dist/cli.js\""
  },
  "dependencies": {
    "@tenk/engine": "latest",
    "canvas": "^2.10.1",
    "cli-progress": "^3.11.2",
    "image-size": "^1.0.2",
    "node-html-parser": "^6.0.0",
    "yargs": "^17.5.1"
  },
  "tsup": {
    "entry": [
      "src/cli.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "bin": {
    "tenk": "dist/cli.js"
  }
}
